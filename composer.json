{
    "name": "quintenmbusiness/laravel-test-core",
    "description": "A streamlined package for creating all types of Laravel tests, including unit, feature, and integration. Simplify test scaffolding with reusable classes.",
    "type": "library",
    "keywords": [
        "laravel",
        "testing",
        "phpunit",
        "feature tests",
        "unit tests",
        "integration tests"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Quintenmbusiness\\LaravelTestCore\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Quinten Muijser",
            "email": "quintenmbusiness@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "laravel/framework": "^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "nunomaduro/larastan": "^2.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
